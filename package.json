    "server/**/*.js": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"

    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.7.4",
    "react-scripts": "^1.0.17",
    "uglify-js": "^2.8.29"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "body-parser": "^1.18.3",
    "bulma": "^0.6.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "date-fns": "^1.29.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "fetch-everywhere": "^1.0.5",
    "ignore-styles": "^5.0.1",
    "izitoast": "^1.2.0",
    "localforage": "^1.5.5",
    "locutus": "^2.0.10",
    "lodash.merge": "^4.6.1",
    "marked": "^0.3.19",
    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-flatpickr": "^3.6.4",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "^4.10.2",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ],
    "server/**/*.js": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "eslint src/**/*.{js,jsx} && prettier --single-quote --no-semi -l src/**/*.{js,jsx,json,css,scss} && prettier --single-quote --no-semi -l server/**/*.js",
    "start": "react-scripts start",
    "start:server": "cross-env NODE_ENV=development node server/index.js",
    "now-start": "cross-env NODE_ENV=production node server/index.js",
    "build": "react-scripts build"
  }
}
